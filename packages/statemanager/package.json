{
  "name": "@ethereumjs/statemanager",
  "version": "1.0.0",
  "description": "An Ethereum statemanager implementation",
  "license": "MPL-2.0",
  "author": "EthereumJS Team",
  "keywords": [
    "ethereum",
    "statemanager"
  ],
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "browser": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": {
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*",
      "types": "./dist/types/*",
      "browser": "./dist/esm/*"
    }
  },
  "scripts": {
    "build": "../../config/cli/ts-build.sh",
    "prepublishOnly": "../../config/cli/prepublish.sh && npm run test:node",
    "clean": "../../config/cli/clean-package.sh",
    "coverage": "../../config/cli/coverage.sh",
    "docs:build": "typedoc --options typedoc.cjs",
    "tsc": "../../config/cli/ts-compile.sh",
    "lint": "../../config/cli/lint.sh",
    "lint:fix": "../../config/cli/lint-fix.sh",
    "tape": "NODE_OPTIONS='--loader ts-node/esm --experimental-json-modules' tape",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "npm run tape -- tests/*.spec.ts",
    "test:browser": "karma start karma.conf.cjs"
  },
  "dependencies": {
    "@ethereumjs/common": "^2.6.3",
    "merkle-patricia-tree": "^4.2.4",
    "debug": "^4.3.3",
    "ethereum-cryptography": "^1.0.3",
    "ethereumjs-util": "^7.1.4",
    "functional-red-black-tree": "^1.0.1",
    "rlp": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/tape": "^4.13.2",
    "eslint": "^8.16.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-tap": "^4.2.0",
    "karma-typescript": "^5.5.3",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "standard": "^10.0.0",
    "tape": "^5.3.1",
    "ts-node": "^10.2.1",
    "typedoc": "^0.22.4",
    "typescript": "^4.7.2"
  },
  "contributors": [
    "g11tech <gajinder@g11.in>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereumjs/ethereumjs-monorepo.git"
  },
  "homepage": "https://github.com/ethereumjs/ethereumjs-monorepo/tree/master/packages/statemanager#readme",
  "bugs": {
    "url": "https://github.com/ethereumjs/ethereumjs-monorepo/issues?q=is%3Aissue+label%3A%22package%3A+statemanager%22"
  }
}
