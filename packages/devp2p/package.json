{
  "name": "@ethereumjs/devp2p",
  "version": "4.2.2",
  "description": "A JavaScript implementation of ÐΞVp2p",
  "license": "MIT",
  "keywords": [
    "ethereum",
    "p2p",
    "networking",
    "dpt",
    "rlpx",
    "eth",
    "les"
  ],
  "engines": {
    "node": ">=12"
  },
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "browser": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": {
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*",
      "types": "./dist/types/*"
    }
  },
  "scripts": {
    "postinstall": "npm run esmHotFixModuleDep && npm run esmHotFixModuleDep2",
    "esmHotFixModuleDep": "npx json -I -f ../../node_modules/@types/ms/package.json -e \"this.types='./index.d.ts'\"",
    "esmHotFixModuleDep2": "npx json -I -f ../../node_modules/@types/ip/package.json -e \"this.types='./index.d.ts'\"",
    "build": "../../config/cli/ts-build.sh",
    "prepublishOnly": "../../config/cli/prepublish.sh",
    "clean": "../../config/cli/clean-package.sh",
    "coverage": "../../config/cli/coverage.sh",
    "docs:build": "typedoc --options typedoc.cjs",
    "tsc": "../../config/cli/ts-compile.sh",
    "lint": "../../config/cli/lint.sh",
    "lint:fix": "../../config/cli/lint-fix.sh",
    "tape": "NODE_OPTIONS='--loader ts-node/esm --experimental-json-modules' tape",
    "test": "npm run tape -- ./test/index.ts"
  },
  "dependencies": {
    "@ethereumjs/common": "^2.6.4",
    "@types/bl": "^2.1.0",
    "@types/k-bucket": "^5.0.0",
    "@types/lru-cache": "^5.1.0",
    "base64url": "^3.0.1",
    "bl": "^1.1.2",
    "debug": "^4.3.3",
    "ethereumjs-util": "^7.1.4",
    "hi-base32": "^0.5.0",
    "inherits": "^2.0.1",
    "ip": "^1.1.3",
    "k-bucket": "^5.0.0",
    "keccak": "^3.0.1",
    "lru-cache": "^5.1.1",
    "ms": "^2.1.3",
    "multiaddr": "^10.0.1",
    "rlp": "^3.0.0",
    "scanf": "^1.1.2",
    "secp256k1": "^4.0.2",
    "snappyjs": "^0.6.1"
  },
  "devDependencies": {
    "@ethereumjs/block": "^3.6.2",
    "@ethereumjs/tx": "^3.5.1",
    "@types/debug": "^4.1.4",
    "@types/ip": "^1.1.0",
    "@types/ms": "^0.7.30",
    "@types/node": "^16.11.7",
    "@types/secp256k1": "^4.0.1",
    "@types/tape": "^4.13.2",
    "chalk": "^5.0.1",
    "eslint": "^8.16.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "tape": "^5.3.1",
    "testdouble": "^3.8.2",
    "typedoc": "^0.22.4",
    "ts-node": "^10.2.1",
    "typescript": "^4.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereumjs/ethereumjs-monorepo.git"
  },
  "homepage": "https://github.com/ethereumjs/ethereumjs-monorepo/tree/master/packages/devp2p#readme",
  "bugs": {
    "url": "https://github.com/ethereumjs/ethereumjs-monorepo/issues?q=is%3Aissue+label%3A%22package%3A+devp2p%22"
  },
  "contributors": [
    "Alex Beregszaszi <alex@rtfs.hu>",
    "Kirill Fomichev <fanatid@ya.ru> (https://github.com/fanatid)",
    "Martin Becze <mb@ethdev.com>",
    "Holger Drewes <holger.drewes@gmail.com>"
  ]
}
