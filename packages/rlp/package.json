{
  "name": "rlp",
  "version": "3.0.0",
  "description": "Recursive Length Prefix Encoding Module",
  "license": "MPL-2.0",
  "keywords": [
    "rlp",
    "ethereum"
  ],
  "type": "module",
  "files": [
    "dist",
    "bin",
    "src"
  ],
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "browser": "./dist/esm/index.js",
  "bin": {
    "rlp": "./bin/rlp.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": {
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*",
      "types": "./dist/types/*"
    }
  },
  "scripts": {
    "build": "../../config/cli/ts-build.sh",
    "prepublishOnly": "../../config/cli/prepublish.sh",
    "clean": "../../config/cli/clean-package.sh",
    "coverage": "../../config/cli/coverage.sh",
    "tsc": "../../config/cli/ts-compile.sh",
    "lint": "../../config/cli/lint.sh",
    "lint:fix": "../../config/cli/lint-fix.sh",
    "tape": "NODE_OPTIONS='--loader ts-node/esm --experimental-json-modules' tape",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "npm run tape -- test/*.spec.ts",
    "test:browser": "karma start karma.conf.cjs"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/tape": "^4.13.2",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.1",
    "karma-tap": "^4.2.0",
    "karma-typescript": "^5.5.3",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "tape": "^5.3.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.7.2"
  },
  "author": {
    "name": "martin becze",
    "email": "mjbecze@gmail.com"
  },
  "contributors": [
    "Alex Beregszaszi <alex@rtfs.hu>",
    "Holger Drewes <Holger.Drewes@gmail.com>",
    "Paul Miller <pkg@paulmillr.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereumjs/ethereumjs-monorepo.git"
  },
  "homepage": "https://github.com/ethereumjs/ethereumjs-monorepo/tree/master/packages/rlp#readme",
  "bugs": {
    "url": "https://github.com/ethereumjs/ethereumjs-monorepo/issues?q=is%3Aissue+label%3A%22package%3A+rlp%22"
  }
}
